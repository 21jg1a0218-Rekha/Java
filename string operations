To perform string operations
package packages;

public class Stringoperations {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str1="kanchana";
		String str2="Rekha";
		String str3=str1+" "+str2;
		System.out.println(str3);
		System.out.println(str1.concat(str2));
		String str4="kanchana rekha     ";
		System.out.println(str4.trim());
		System.out.println(str1.toLowerCase());
		System.out.println(str1.toUpperCase());
		System.out.println(str2.isEmpty());
		String str5="ABCDE";
		String str6="ABCDE";
		String str7="ABCDe";
		System.out.println(str5.equals(str6));
		System.out.println(str6.equalsIgnoreCase(str7));
		System.out.println(str5.compareTo(str6));
		System.out.println(str5.compareToIgnoreCase(str7));
		String pattern="[A-Z] {1,}";
		System.out.println(str5.matches(pattern));





	}

}
